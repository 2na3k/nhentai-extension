{ 
  "manifest_version": 3,
  "name": "Nhentai Extension",
  "version": "0.0.1",
  "description": "Quick access Nhentai by doing things",
  "permissions": ["tabs"],

  

  "action": {
    "default_icon": {
      "16": "assets/icon.jpg",
      "24": "assets/icon.jpg",
      "32": "assets/icon.jpg"
    },

    "default_title": "Nhentai quick access",
    "default_popup": "main.html"
  },
  "commands": {
	"_execute_action": {
    	    "suggested_key": {
                "windows": "Ctrl+Shift+F",
                "mac": "Command+Shift+F"
            }
        }
    }
}